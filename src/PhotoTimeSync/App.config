<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="logConfig" type="PhotoTimeSync.Logging.Configuration.LogConfig, PhotoTimeSync"/>
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <logConfig>
    <rotatingLogs>
      <rotatingLog baseFilePath="F:\Temp\PhotoTimeSync.log" maxLogEntriesPendingInMemory="10" nbRotatingFiles="5" eachFileMaxSize="500000">
        <switches>
          <switch displayName="All Verbose" description="Log all verbose or above in rotating file" level="Verbose"/>
        </switches>
      </rotatingLog>
    </rotatingLogs>
  </logConfig>
</configuration>